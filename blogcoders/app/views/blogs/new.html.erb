<h1>New post</h1>
 
<% form_for :post , :url => blogs_path do |f| -%>
    <%= f.error_messages %>
    <div><label>title</label><%= f.text_field :title %></div>
    <div><label>content</label><%= f.text_area :content %> </div>
    <%= f.submit "Submit", :disable_with => 'Submiting...' %>
<% end -%>
 
 
<%= link_to 'Back', blogs_path %> 
